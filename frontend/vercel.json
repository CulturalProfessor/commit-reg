{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "redirects": [
    {
      "source": "/commit",
      "destination": "https://ossrndc.in/"
    },
    {
      "source": "/redirect",
      "destination": "/redirect"
    },
    {
      "source": "ossrndc.in/commit",
      "destination": "https://ossrndc.in"
    }
  ]
}
